{
    "workbench.iconTheme": "material-icon-theme",
    "workbench.startupEditor": "none",
    "cmake.configureOnOpen": true,
    "git.openRepositoryInParentFolders": "never",
    "explorer.confirmDelete": false,
    "redhat.telemetry.enabled": true,
    "explorer.confirmDragAndDrop": false,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorStyleInactive": "line",
    "terminal.integrated.cursorWidth": 2,
    "editor.detectIndentation": false,
    "security.workspace.trust.untrustedFiles": "open",
    "debug.disassemblyView.showSourceCode": false,
    "files.autoSave": "onFocusChange",
    "arduino.useArduinoCli": true,
    "terminal.integrated.fontFamily": "monospace",
    "editor.fontFamily": "SF mono",
    "editor.fontSize": 13,
    "liveServer.settings.ChromeDebuggingAttachment": false,
    "[css]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "liveServer.settings.donotShowInfoMsg": true,
    "explorer.confirmPasteNative": false,
    "workbench.colorCustomizations": {
        "editor.background": "#141010",
        "editor.selectionBackground": "#36535E",
        "editor.findMatchBackground": "#893747",
        "editor.hoverHighlightBackground": "#024959",
        "editor.lineHighlightBackground": "#141010",
        "editor.lineHighlightBorder": "#024959",
        "editorWidget.border": "#DED310",
        "editorWidget.background": "#012E40",
        "editorBracketMatch.border": "#DED310",
        "editorGroupHeader.tabsBorder": "#DED310",
        "widget.shadow": "#F2E3D5",
        "input.background": "#141010",
        "button.background": "#026773",
        "editorLineNumber.activeForeground": "#DED310",
        "editorLineNumber.foreground": "#026773",
        "list.hoverBackground": "#024959", 
        "tab.hoverBackground": "#024959",  
        "list.focusHighlightForeground": "#10d0de",
        "quickInputList.focusBackground": "#024959",
        "quickInputList.focusForeground": "#f9f6f2",
        "editorCursor.foreground": "#DED310",
        "sideBar.foreground": "#F2E3D5",
        "sideBarSectionHeader.background": "#012E40",
        "sideBarSectionHeader.foreground": "#DE1038",
        "sideBarTitle.foreground": "#DE1038",
        "titleBar.activeBackground": "#012E40",
        "titleBar.activeForeground": "#DED310",
        "titleBar.border": "#DED310",
        "titleBar.inactiveBackground": "#012E40",
        "tab.activeBorder": "#F2E3D5",
        "panelTitle.activeForeground": "#DED310",
        "panelTitle.activeBorder": "#026773",
        "panelTitle.inactiveForeground": "#026773",
        "activityBar.inactiveForeground": "#026773",
        "activityBar.foreground": "#DED310",
        "activityBar.activeBorder": "#026773",
        "activityBar.background": "#141010"
    },
    "editor.tokenColorCustomizations": {
        "variables": "#FC08BF",
        "functions": "#35E6DF",
        "keywords": "#DE1038",
        "comments": "#026773",
        "types": "#FE5501",
        "strings": "#009407",
        "numbers": "#F2E3D5",
        "textMateRules": [{
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#DED310"
            }
        }]
    }
}